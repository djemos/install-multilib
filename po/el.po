#: src/install-multilib:46
#, sh-format
msgid "USAGE: install-multilib [salix|slackel] [local|download]"
msgstr "ΧΡΗΣΗ: install-multilib [salix|slackel] [local|download]"

#: src/install-multilib:48
#, sh-format
msgid "OPTIONS:"
msgstr "ΕΠΙΛΟΓΕΣ:"

#: src/install-multilib:49
#, sh-format
msgid "   salix,   Install multilib for all Salix versions"
msgstr "   salix,   εγκατάσταση της multilib για όλες τις εκδόσεις του Salix"

#: src/install-multilib:50
#, sh-format
msgid "   slackel, Install multilib for slackel current"
msgstr ""
"   slackel, εγκατάσταση της multilib για όλες τις εκδόσεις του  slackel "
"current"

#: src/install-multilib:51
#, sh-format
msgid "   local,   Installing from the local repo"
msgstr "   local,   εγκατάσταση από το τοπικό αποθετήριο"

#: src/install-multilib:52
#, sh-format
msgid "   download,  Download the packages, converting and installing them..."
msgstr "download,  Μεταφόρτωση πακέτων, μετατροπή και εγκατάσταση αυτών..."

#: src/install-multilib:53
#, sh-format
msgid ""
"================================== ATTENSION ==============================="
msgstr ""
"================================== ΠΡΟΣΟΧΗ ==============================="

#: src/install-multilib:54
#, sh-format
msgid ""
"Please add in blacklist gcc* and glibc*, for not upgrade them with slapt-get"
msgstr ""
"Παρακαλώ προσθέστε στη blacklist το gcc* και glibc*, για να μην ενημερωθούν "
"με το slapt-get"

#: src/install-multilib:55
#, sh-format
msgid "============================================================================"
msgstr "============================================================================"

#: src/install-multilib:65
#, sh-format
msgid "ERROR: Too many arguments"
msgstr "ΛΑΘΟΣ: Πάρα πολλές παράμετροι"

#: install-multilib.desktop.in.h:1
msgid "install multilib"
msgstr "Εγκατάσταση της multilib"
